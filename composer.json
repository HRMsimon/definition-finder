{
  "name": "fredemmott/definition-finder",
  "description": "Find definitions in PHP or Hack files. Useful for autoloaders.",
  "keywords": ["hack", "definitions", "hhvm", "autoload"],
  "homepage": "https://github.com/fredemmott/definitions-finder",
  "license": ["BSD-3-Clause"],
  "require": {
    "hhvm": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "files": [
      "src/typedefs.php",
      "src/utils.php"
    ]
  }
}
